{
    "apiVersion": "v1beta1",
    "desiredState": {
        "podTemplate": {
            "desiredState": {
                "manifest": {
                    "containers": [
                        {
                            "env": [
                                {
                                    "name": "RACK_ENV",
                                    "value": "production"
                                }
                            ],
                            "image": "waloeiii/andrewloe-com",
                            "name": "andrewloe-com",
                            "ports": [
                                {
                                    "containerPort": 80,
                                    "hostPort": 9292
                                }
                            ]
                        }
                    ],
                    "id": "andrewloe-com-frontend",
                    "version": "v1beta1"
                }
            },
            "labels": {
                "app": "andrewloe-com",
                "name": "andrewloe-com-frontend"
            }
        },
        "replicaSelector": {
            "name": "andrewloe-com-frontend"
        },
        "replicas": 1
    },
    "id": "andrewloe-com-controller",
    "kind": "ReplicationController",
    "labels": {
        "app": "andrewloe-com",
        "name": "andrewloe-com-rc"
    }
}
